package javaweek1assignment;

import java.util.Scanner;

public class EggsQuestion {

	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		System.out.println("Enter eggs number: ");
		int i =sc.nextInt();
		int j = i/144;
		int l = i%144;
		
		int a = l/12;
		int b = l%12;
		
		System.out.println("number of eggs: " + j + " = gross"+" " +" "+ a + " = dozen"+" " + b + " = other");
		sc.close();
		

	}

}
